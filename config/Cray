#define BuildXlibDependentCode
#define BuildClearTextctrans
#define BuildUnsupportedAthenaApplications

VERSION         = 3.1.3
PARFIL_INIT     = /usr/local/lib/ncargparams
PARFIL_PERM     = /usr/local/lib/ncargparams

BINDIR          = /usr/local/bin
LIBDIR          = /usr/local/lib
MANDIR          = /usr/man/manl
MAN_SECTION     = l
TMPDIR          = /usr/tmp

XLIBDIR         = /usr/lib
XINCDIR         = /usr/include
XAPPDIR         = /usr/lib/X11/app-defaults

INCLUDEDIR      = $(NCARG)/include
DBDIR           = $(LIBDIR)
FONTCAPDIR      = $(LIBDIR)/fontcaps
GRAPHCAPDIR     = $(LIBDIR)/graphcaps
EXAMPLEDIR      = $(LIBDIR)/ncargexamples
TESTDIR         = $(LIBDIR)/ncargtests

GENERICDIR      = $(NCARG)/generic

CONFIGDIR       = $(NCARG)/config
YMAKE           = $(CONFIGDIR)/ymake -I$(CONFIGDIR)

MAKE            = make
FLOAT_OPT       = 
OS              = -DSYSV
ARCH            =
XPATH           =
VDEFINE         = -DVERSION='"$(VERSION)"'
CC              = cc
CC_LD           = $(CC)
CDEBUG          =
COPT            = -h noopt
CMISC           = -DUSG
CFLAGS          = $(OS) $(ARCH) $(CDEFINES) $(CDEBUG) $(COPT) $(CMISC)\
                  $(FLOAT_OPT) $(XPATH) $(VDEFINE)
LD_CFLAGS       = -d FORCE=OFF $(CDEBUG)
CPP             = /lib/cpp
FCPP            = $(NCARG)/utils/fcpp $(CPP)
F77             = cf77
F77_LD          = $(F77)
FDEBUG          =
FOPT            =
FMISC           =
FFLAGS          = $(ARCH) $(FDEFINES) $(FDEBUG) $(FOPT) $(FMISC)\
                  $(FLOAT_OPT)
LD_FFLAGS       = -Wl"-D FORCE=OFF $(FDEBUG)"
LD              = ld
LD_REL          = $(NCARG)/utils/ldrel
AR              = bld
AR_QUICK        = $(AR) q
AR_CHECK        = $(AR) vr
AR_SRC          = ar
AR_SRC_QUICK    = $(AR_SRC) q
AR_SRC_CHECK    = $(AR_SRC) vr
RM              = rm -f
CAT             = cat
CP              = cp
MV              = mv -f
RANLIB          = $(NO_OP)
NO_OP           = touch -c
INSTALL         = $(NCARG)/utils/install -c
INSTALL_RM      = $(RM)
INSTALL_BIN     = 0755
INSTALL_MAN     = 0644
INSTALL_APP     = 0644
INSTALL_LIB     = 0644
INSTALL_DB      = 0644

GRAPHC          = $(BINDIR)/graphc
FONTC           = $(BINDIR)/fontc

CALLCONV        = $(NCARG)/utils/callconv
SHELL           = /bin/sh

GKS_BUFFER_SIZE = 1024
