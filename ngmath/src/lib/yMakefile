#define IHaveSubdirs

SUBDIRS = gridpack

INCLUDES = ngmath.h

PartialLibraryRoot(libngmath.a,$(SUBDIRS))
BuildSharedFromArch(libNGmath,libngmath.a,1,0,$(CTOFLIBS) -lm -lc)
InstallTarget($(INCLUDES),$(INSTALL_INC),$(NCARGINCPATH))
BuildIncludes($(INCLUDES),ncarg,..)

