MYNAME	= natgrid

GENERIC_SRCS = \
natgridd.c      nncrunchd.c     nnerror.c       nnusers.c
natgrids.c      nncrunchs.c     nnuserd.c

GENERIC_OBJS = \
natgridd.o      nncrunchd.o     nnerror.o       nnusers.o
natgrids.o      nncrunchs.o     nnuserd.o

SOURCES = $(GENERIC_SRCS)

OBJECTS = $(GENERIC_OBJS)

OTHER_SRCS = nnuser_f.f

OTHER_OBJS = nnuser_f.f

INCLUDES = nncheadd.h nncheads.h nngheadd.h nngheads.h nnuheadd.h nnuheads.h

PartialLibraryTarget(libngmath.a,$(OBJECTS))

natgrids.o  : natgrids.c nngheads.h
natgridd.o  : natgridd.c nngheadd.h
nncrunchs.o : nncrunchs.c nncheads.h nnuheads.h
nncrunchd.o : nncrunchd.c nncheadd.h nnuheadd.h
nnusers.o   : nnusers.c nnuheads.h
nnuserd.o   : nnuserd.c nnuheadd.h
nnerror.o   : nnerror.c
nnuser_f.o  : nnuser_f.f
