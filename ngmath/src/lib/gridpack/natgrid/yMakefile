MYNAME  = natgrid

SOURCES = natgridd.c nncrunchd.c nnerror.c nnusers.c natgrids.c nncrunchs.c \
                        nnuserd.c nnuser_f.f

OBJECTS = natgridd.o nncrunchd.o nnerror.o nnusers.o natgrids.o nncrunchs.o \
                        nnuserd.o nnuser_f.o

