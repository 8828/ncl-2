	c_ftex01.o   c_ftex02.o   c_ftex03.o     \
	c_ftex04.o   c_ftex05.o   c_ftex06.o     \
	c_ftex07.o                               \
	ftex01.o     ftex02.o     ftex03.o       \
	ftex04.o     ftex05.o     ftex06.o       \
	ftex07.o

