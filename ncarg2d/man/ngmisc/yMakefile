MAN = ngdots nggcog nggetc nggeti nggetr nggsog ngmft ngmisc ngpict ngpswk \
      ngreop ngritd ngsetc ngseti ngsetr ngsrat ngwsym ngmisc_params

# Install preformatted troff documents on systems without troff
#if UseCatDirs
FORMAT_EXT	= .p
#else
FORMAT_EXT	= .m
#endif

InstallManPages($(MAN),$(FORMAT_EXT),$(MAN3PATH),$(MAN3_SECTION))
FormatManPages($(MAN),.m,.p)
