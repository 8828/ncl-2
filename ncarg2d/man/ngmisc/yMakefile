MAN = ngdots nggcog nggetc nggeti nggetr nggsog ngmisc ngpict ngpswk ngritd \
      ngsetc ngseti ngsetr ngwsym ngmisc_params

# Install preformatted troff documents on systems without troff
#if UseCatDirs
FORMAT_EXT	= .p
#else
FORMAT_EXT	= .m
#endif

InstallManPages($(MAN),$(FORMAT_EXT),$(MAN3PATH),$(MAN3_SECTION))
FormatManPages($(MAN),.m,.p)
