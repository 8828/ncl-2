#

MAN = colconv hlsrgb hsvrgb rgbhls rgbhsv rgbyiq yiqrgb

# Install preformatted troff documents on systems without troff
#if UseCatDirs
FORMAT_EXT	= .p
#else
FORMAT_EXT	= .m
#endif

MPATH	= $(MAN3PATH)
MSECT	= $(MAN3_SECTION)

InstallManPages($(MAN),$(FORMAT_EXT),$(MPATH),$(MSECT))
FormatManPages($(MAN),.m,.p)
