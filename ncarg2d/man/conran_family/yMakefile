MAN = conop1 conop2 conop3 conop4 conran conran_family conran_family_params \
	conraq conras

# Install preformatted troff documents on systems without troff
#ifdef UseCatDirs
FORMAT_EXT	= .p
#else
FORMAT_EXT	= .m
#endif

MPATH	= $(MAN3PATH)
MSECT	= $(MAN3_SECTION)

InstallManPages($(MAN),$(FORMAT_EXT),$(MPATH),$(MSECT))
FormatManPages($(MAN),.m,.p)
