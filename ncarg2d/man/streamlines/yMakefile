MAN =	ezstrm stgeti stgetr stinit stream streamlines streamlines_params \
	strmln strset stseti stsetr stuixy stumsl stumta stumxy

# Install preformatted troff documents on systems without troff
#if UseCatDirs
FORMAT_EXT	= .p
#else
FORMAT_EXT	= .m
#endif

InstallManPages($(MAN),$(FORMAT_EXT),$(MAN3PATH),$(MAN3_SECTION))
FormatManPages($(MAN),.m,.p)
