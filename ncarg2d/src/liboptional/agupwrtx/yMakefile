NCARG	= ../../../..

MYNAME	= agupwrtx

GENERIC_SRCS = agpwrt.f

GENERIC_OBJS = agpwrt.o

OBJECTS = $(GENERIC_OBJS)

SOURCES = $(GENERIC_SRCS)

RelocatableObject(libagupwrtx.o,$(OBJECTS))

#ifdef  BuildNCARGSourceLibrary
install:: $(SOURCES)
	@-$(RM) $(LIBPATH)/srcagupwrtx.a
	@echo "$(SOURCES)" > $(MYNAME)
	@$(AR_SRC_QUICK) $(LIBPATH)/srcagupwrtx.a $(MYNAME) $(SOURCES)
	$(RANLIB) $(LIBPATH)/srcagupwrtx.a
	@-$(RM) $(MYNAME)
#endif
