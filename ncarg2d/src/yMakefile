NCARG	= ../..

SUBDIRS = libncarg libncarg_gks liboptional bin db examples fundamental tests \
	tutorial libncargC libncarg_gksC examplesC

MakeSubdirs($(SUBDIRS))
InstallSubdirs($(SUBDIRS))
MakefilesSubdirs($(SUBDIRS))
CleanSubdirs($(SUBDIRS))
