NCARG	= ../..

SUBDIRS = libncarg libncarg_gks liboptional libncargC libncarg_gksC bin db \
	examples fundamental tests tutorial examplesC pdocexamples

MakeSubdirs($(SUBDIRS))
InstallSubdirs($(SUBDIRS))
MakefilesSubdirs($(SUBDIRS))
CleanSubdirs($(SUBDIRS))
