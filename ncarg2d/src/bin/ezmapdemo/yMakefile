MYNAME  = ezmapdemo

SRC    = EzmapDemo.f
OBJ    = EzmapDemo.o

PROG_LIBS =	$(NCARGLIBS)
DEP_LIBS =	$(DEPNCARGLIBS)
SYS_LIBS	= $(XLIB)

#if defined(Darwin)
#EXTRA_FCOPTIONS  = -N11
#elif defined(AIX)
EXTRA_FCOPTIONS  = -NS32704
#endif

FortranProgram($(MYNAME),$(OBJ),$(DEP_LIBS))
InstallTarget($(SRC),$(INSTALL_LIB),$(EXAMPLESPATH))
