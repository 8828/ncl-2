NCARG	= ../../../..

MYNAME	= impltest

#if defined(ardent) || (defined(u370) && defined(unix))
OTHER_LIBS =	$(LIBPATH)/libncarbd.o
#endif

COMMON_LIBS =	$(LIBPATH)/libncarg.a \
		$(LIBPATH)/libncarg_gks.a \
		$(LIBPATH)/libncarg_c.a \
		$(LIBPATH)/libncarg_loc.a

ROBJS = $(ROBJPATH)/ggkwdr_stub.o
LIBS = $(ROBJS) $(OTHER_LIBS) $(COMMON_LIBS)

GENERIC_SRCS = \
	 lcm.f mskout.f implbd.f tgbyte.f tgks0a.f tiand.f\
	 tior.f tlocal.f tsbyte.f tshift.f

GENERIC_OBJS = \
	 lcm.o mskout.o implbd.o tgbyte.o tgks0a.o tiand.o\
	 tior.o tlocal.o tsbyte.o tshift.o

#ifdef cray

TLOCAL_OBJS = \
	tlocalcray.o tiand.o tior.o lcm.o tgbyte.o tshift.o tsbyte.o\
	mskout.o

#else
#ifdef ardent

TLOCAL_OBJS = \
	tlocalardent.o tiand.o tior.o lcm.o tgbyte.o tshift.o tsbyte.o\
	mskout.o

#else
#if defined(u370) && defined(unix) 

TLOCAL_OBJS = \
	tlocalibm.o tiand.o tior.o lcm.o tgbyte.o tshift.o tsbyte.o\
	mskout.o

#else

TLOCAL_OBJS = \
	tlocal32.o tiand.o tior.o lcm.o tgbyte.o tshift.o tsbyte.o\
	mskout.o

#endif
#endif
#endif


all:: tlocal tgks0a

FortranTarget(tlocal,$(TLOCAL_OBJS),$(LIBS))
FortranTarget(tgks0a,tgks0a.o,$(LIBS))
MakefileTarget
CleanTarget
UpdateTarget
