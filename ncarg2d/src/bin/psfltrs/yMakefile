MYNAME  = psfltrs

GENERIC_SRCS = ps.bkg_black.f ps.bkg_white.f
GENERIC_OBJS = ps.bkg_black.o ps.bkg_white.o

PROG_LIBS = $(NCARGLOCLIB)
DEP_LIBS = $(DEPNCARGLOCLIB)

FortranProgram(psblack,ps.bkg_black.o,$(DEP_LIBS))
FortranProgram(pswhite,ps.bkg_white.o,$(DEP_LIBS))
