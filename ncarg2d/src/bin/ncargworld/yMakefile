MYNAME	= ncargworld

SRCS    = ncargworld.f
OBJS    = ncargworld.o

#ifdef	UseInstalled
ROBJS = $(ROBJROOT)/ggkwdr_stub.o
#else
ROBJS = $(NCARGGKSLIBSRC)/awi/ggkwdr_stub.o
#endif

OBJECTS	= $(OBJS) $(ROBJS) $(OTHER_ROBJS)

#if defined(Ardent) || defined(AIX370)
#ifdef	UseInstalled
OTHER_ROBJS =	$(ROBJROOT)/libncarbd.o
#else
OTHER_ROBJS =	$(NCARGOPTLIBSRC)/ncarbd/ncarbd.o
#endif	/* UseInstalled */
#endif	/* Ardent || AIX370 */

PROG_LIBS =	$(NCARGLIBS)
DEP_LIBS =	$(DEPNCARGLIBS)
SYS_LIBS	=	-lm

FortranProgram($(MYNAME),$(OBJECTS),$(DEP_LIBS))
InstallTarget($(SRCS),$(INSTALL_LIB),$(EXAMPLESPATH))
