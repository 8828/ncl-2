#

MYNAME	= ncargworld

SRC    = ncargworld.f
MSRC   = ncargworldm.f
MOBJ    = ncargworldm.o

#ifdef	UseInstalled
ROBJS = $(ROBJROOT)/ggkwdr_stub.o
#else
ROBJS = $(NCARGGKSLIBSRC)/awi/ggkwdr_stub.o
#endif

OBJECTS	= $(MOBJ) $(ROBJS) $(OTHER_ROBJS)

#if defined(Ardent) || defined(AIX370)
#ifdef	UseInstalled
OTHER_ROBJS =	$(ROBJROOT)/libncarbd.o
#else
OTHER_ROBJS =	$(NCARGOPTLIBSRC)/ncarbd/ncarbd.o
#endif	/* UseInstalled */
#endif	/* Ardent || AIX370 */

PROG_LIBS =	$(NCARGLIBS)
DEP_LIBS =	$(DEPNCARGLIBS)
SYS_LIBS	=	-lm

FortranProgram($(MYNAME),$(OBJECTS),$(DEP_LIBS))
InstallTarget($(SRC),$(INSTALL_LIB),$(EXAMPLESPATH))

ncargworldm.f: ncargworld.f Makefile
	@sed \
	-e 's+SED_WSTYPE+1+' \
	< ncargworld.f > ncargworldm.f

CleanFilesTarget($(MSRC) $(MOBJ))

