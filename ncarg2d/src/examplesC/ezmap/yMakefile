SRCS = c_eezmpa.c c_mpex05.c
OBJS = c_eezmpa.o c_mpex05.o

PROG_LIBS		= $(CNCARGLIBS) $(NCARGLIBS)
SYS_LIBS		= $(CTOFLIBS) -lm
EXTRA_LIBS		= $(XLIB)
EXTRA_DEFINES	= -DSED_WSTYPE=1

InstallTarget($(SRCS),$(INSTALL_LIB),$(TESTSPATH))

CExampleTarget(c_eezmpa,c_eezmpa.o)
CExampleTarget(c_mpex05,c_mpex05.o)
