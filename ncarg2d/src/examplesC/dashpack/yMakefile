SRCS = c_tdshpk.c
OBJS = c_tdshpk.o

PROG_LIBS		= $(CNCARGLIBS) $(NCARGLIBS)
SYS_LIBS		= $(CTOFLIBS) -lm
EXTRA_LIBS		= $(XLIB)
EXTRA_DEFINES	= -DSED_WSTYPE=1

InstallTarget($(SRCS),$(INSTALL_LIB),$(TESTSPATH))

CExampleTarget(c_tdshpk,c_tdshpk.o)
