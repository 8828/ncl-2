SRCS = c_fdldashc.c
OBJS = c_fdldashc.o

PROG_LIBS		= $(CNCARGLIBS) $(NCARGLIBS)
SYS_LIBS		= $(CTOFLIBS) -lm
EXTRA_LIBS		= $(XLIB)
EXTRA_DEFINES	= -DSED_WSTYPE=1

InstallTarget($(SRCS),$(INSTALL_LIB),$(EXAMPLESPATH))

CExampleTarget(c_fdldashc,c_fdldashc.o)
