CSRCS = c_tvelvc.c c_ffex02.c

SED_SRCS = c_fcover.c c_ffex05.c c_vvex01.c c_vvex02.c c_vvex03.c

GENERIC_SRCS = $(CSRCS) $(SED_SRCS)

InstallTarget($(GENERIC_SRCS),$(INSTALL_LIB),$(EXAMPLESPATH))
CleanFilesTarget($(SED_SRCS))

c_fcover.c: c_fcover.c.sed Makefile
	@-$(RM) $@
	@$(CALLCONV) $(SYSTEM_INCLUDE) $@ vvudmv

c_ffex05.c: c_ffex05.c.sed Makefile
	@-$(RM) $@
	@$(CALLCONV) $(SYSTEM_INCLUDE) $@ vvudmv

c_vvex01.c: c_vvex01.c.sed Makefile
	@-$(RM) $@
	@$(CALLCONV) $(SYSTEM_INCLUDE) $@ vvudmv gngpat

c_vvex02.c: c_vvex02.c.sed Makefile
	@-$(RM) $@
	@$(CALLCONV) $(SYSTEM_INCLUDE) $@ gngpat

c_vvex03.c: c_vvex03.c.sed Makefile
	@-$(RM) $@
	@$(CALLCONV) $(SYSTEM_INCLUDE) $@ vvmap vvumxy
