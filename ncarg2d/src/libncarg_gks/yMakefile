#define	IHaveSubdirs

SUBDIRS = awi bwi wiss cdrivers

PartialLibraryRoot(libncarg_gks.a,$(SUBDIRS))
PartialSourceLibraryRoot(srcncarg_gks.a,$(SUBDIRS))

BuildSharedFromArch(libNGgks,libbasegks.a,1,0,-delay_load -lNGgksCTXT -delay_load -lNGgksX -delay_load -lNGgksPS -delay_load $(NCARGCLIB) $(CTOFLIBS) -lm -lc)
