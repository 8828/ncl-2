#
#	Makefile for the NCAR GKS library
#

NCARG	= ../../..

SUBDIRS = awi bwi wiss X11

#ifdef  BuildNCARGSourceLibrary
all::
	@-$(RM) $(LIBPATH)/srcncarg_gks.a
#endif

PartialLibraryRoot(libncarg_gks.a,$(SUBDIRS))

#ifdef  BuildNCARGSourceLibrary
all::
	$(RANLIB) $(LIBPATH)/srcncarg_gks.a
#endif
