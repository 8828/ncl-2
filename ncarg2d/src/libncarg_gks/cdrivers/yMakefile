MYNAME		= cdrivers

SOURCES		= \
		ctxt.c ctxt_conv.c font.c fort_c.c gks_device.c gksc.c \
		text.c transform.c x_conv.c xattribute.c xcontrol.c \
		ps.c ps_conv.c psfill.c \
		xinquiry.c xoutput.c

OBJECTS		= \
		ctxt.o ctxt_conv.o font.o fort_c.o gks_device.o gksc.o \
		text.o transform.o x_conv.o xattribute.o xcontrol.o \
		ps.o ps_conv.o psfill.o \
		xinquiry.o xoutput.o

EXTRA_CDEFINES	= $(OS_DEF) -DCTXT -DX11 -DPS

PartialLibraryTarget(libncarg_gks.a,$(OBJECTS))
PartialSourceLibraryTarget($(MYNAME),srcncarg_gks.a,$(SOURCES))
DependTarget($(SOURCES))
