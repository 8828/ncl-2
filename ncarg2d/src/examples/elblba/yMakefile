NCARG	= ../../../..

MYNAME	= elblba

GENERIC_SRCS = elblba.f

GENERIC_OBJS = elblba.o

install::
	@for file in $(GENERIC_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLEDIR));\
	done

MakefileTarget
CleanTarget
UpdateNoSplitTarget
