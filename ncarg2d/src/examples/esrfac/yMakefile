NCARG	= ../../../..

FSPLIT	= ncarguncat

MYNAME	= esrfac

GENERIC_SRCS = \
	srex01.f srex01.dat

GENERIC_OBJS = \
	srex01.o srex01.dat

install::
	@for file in $(GENERIC_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLEDIR));\
	done

MakefileTarget
CleanTarget
UpdateTarget
