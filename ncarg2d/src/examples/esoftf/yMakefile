NCARG	= ../../../..

FSPLIT	= ncarguncat

MYNAME	= esoftf

GENERIC_SRCS = \
	 sfex01.f sfex02.f

GENERIC_OBJS = \
	 sfex01.o sfex02.o

install::
	@for file in $(GENERIC_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLEDIR));\
	done

MakefileTarget
CleanTarget
UpdateTarget
