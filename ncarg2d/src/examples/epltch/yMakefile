NCARG	= ../../../..

MYNAME	= epltch

GENERIC_SRCS = epltch.f

GENERIC_OBJS = epltch.o

install::
	@for file in $(GENERIC_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLEDIR));\
	done

MakefileTarget
CleanTarget
UpdateNoSplitTarget
