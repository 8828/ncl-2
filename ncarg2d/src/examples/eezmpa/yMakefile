NCARG	= ../../../..

MYNAME	= eezmpa

GENERIC_SRCS = eezmpa.f

GENERIC_OBJS = eezmpa.o

install::
	@for file in $(GENERIC_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLEDIR));\
	done

MakefileTarget
CleanTarget
UpdateNoSplitTarget
