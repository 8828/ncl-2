MYNAME  = etpack

SRCS = tdex01.f tdex02.f tdex03.f
OBJS = tdex01.o tdex02.o tdex03.o

PROG_LIBS		= $(NCARGLIBS)
SYS_LIBS		= -lm
EXTRA_LIBS		= $(XLIB)

InstallTarget($(SRCS),$(INSTALL_LIB),$(EXAMPLESPATH))

FExampleTarget(tdex01,tdex01.o)
FExampleTarget(tdex02,tdex02.o)
FExampleTarget(tdex03,tdex03.o)
