NCARG	= ../../../..

FSPLIT	= ncarguncat

MYNAME	= emisc

GENERIC_SRCS = \
	 arex01.f coex01.f coex02.f coex03.f cbex01.f stex01.f

GENERIC_OBJS = \
	 arex01.o coex01.o coex02.o coex03.o cbex01.o stex01.o

OTHER_SRCS = example.f

install::
	@for file in $(GENERIC_SRCS) $(OTHER_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLESPATH));\
	done

MakefileTarget
CleanTarget
UpdateTarget
