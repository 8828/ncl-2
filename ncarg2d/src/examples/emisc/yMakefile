NCARG	= ../../../..

FSPLIT	= ncarguncat

MYNAME	= emisc

GENERIC_SRCS = \
	 bnchmk.f example.f

GENERIC_OBJS = \
	 bnchmk.o example.o

install::
	@for file in $(GENERIC_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLESPATH));\
	done

MakefileTarget
CleanTarget
UpdateTarget
