MYNAME	= emisc

SRCS = bnchmk.f example.f
OBJS = bnchmk.o example.o

PROG_LIBS		= $(NCARGLIBS)
SYS_LIBS		= -lm
EXTRA_LIBS		= $(XLIB)
EXTRA_DEFINES	= -DSED_WSTYPE=1

InstallTarget($(SRCS),$(INSTALL_LIB),$(EXAMPLESPATH))

FExampleTarget(bnchmk,bnchmk.o)
FExampleTarget(example,example.o)
