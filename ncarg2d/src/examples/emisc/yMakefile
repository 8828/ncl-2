NCARG	= ../../../..

FSPLIT	= ncarguncat

MYNAME	= emisc

GENERIC_SRCS = \
	 arex01.f coex01.f coex02.f coex03.f cbex01.f slex01.f bnchmk.f

GENERIC_OBJS = \
	 arex01.o coex01.o coex02.o coex03.o cbex01.o slex01.o bnchmk.o

OTHER_SRCS = example.f

install::
	@for file in $(GENERIC_SRCS) $(OTHER_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLESPATH));\
	done

MakefileTarget
CleanTarget
UpdateTarget
