NCARG	= ../../../..

FSPLIT	= ncarguncat

MYNAME	= estitl

GENERIC_SRCS = \
	 slex01.f

GENERIC_OBJS = \
	 slex01.o

install::
	@for file in $(GENERIC_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLESPATH));\
	done

MakefileTarget
CleanTarget
UpdateTarget
