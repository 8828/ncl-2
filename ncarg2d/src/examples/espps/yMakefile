NCARG	= ../../../..

FSPLIT	= ncarguncat

MYNAME	= espps

GENERIC_SRCS = \
	splogy.f sprevx.f

install::
	@for file in $(GENERIC_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLESPATH));\
	done

MakefileTarget
CleanTarget
UpdateTarget
