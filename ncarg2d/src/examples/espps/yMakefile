MYNAME	= espps

SRCS = splogy.f sprevx.f
OBJS = splogy.o sprevx.o

PROG_LIBS		= $(NCARGLIBS)
SYS_LIBS		= -lm
EXTRA_LIBS		= $(XLIB)
EXTRA_DEFINES	= -DSED_WSTYPE=1

InstallTarget($(SRCS),$(INSTALL_LIB),$(EXAMPLESPATH))

FExampleTarget(splogy,splogy.o)
FExampleTarget(sprevx,sprevx.o)
