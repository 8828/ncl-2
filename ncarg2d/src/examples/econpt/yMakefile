MYNAME  = econpt

SRCS =  \
	ctex01.f ctex02.f ctfite.f ctgaus.f ctgeo1.f ctgeo2.f ctiscp.f \
	ctllg1.f ctllg2.f ctllg3.f ctorca.f ctpopg.f ctswth.f ctwng1.f \
	ctwng2.f \
	ctfite.dat ctgaus.dat ctiscp.dat ctorca.dat ctpopg.dat ctswth.dat \
	ctwng1.dat ctwng2.dat

OBJS =  \
	ctex01.o ctex02.o ctfite.o ctgaus.o ctgeo1.o ctgeo2.o ctiscp.o \
	ctllg1.o ctllg2.o ctllg3.o ctorca.o ctpopg.o ctswth.o ctwng1.o \
	ctwng2.o

PROG_LIBS		= $(NCARGLIBS)
SYS_LIBS		= -lm
EXTRA_LIBS		= $(XLIB)

InstallTarget($(SRCS),$(INSTALL_LIB),$(EXAMPLESPATH))

FExampleTarget(ctex01,ctex01.o)
