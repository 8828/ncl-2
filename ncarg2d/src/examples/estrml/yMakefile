NCARG	= ../../../..

FSPLIT	= ncarguncat

MYNAME	= estrml

GENERIC_SRCS = \
	stex01.f stex02.f stex03.f

install::
	@for file in $(GENERIC_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLESPATH));\
	done

MakefileTarget
CleanTarget
UpdateTarget
