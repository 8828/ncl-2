#
#	$Id: yMakefile,v 1.1.1.1 1992-04-17 22:31:34 ncargd Exp $
#
NCARG	= ../../../..

MYNAME	= pwrity

GENERIC_SRCS = \
	 pwrity.f pwry.f pwrybd.f pwrygt.f pwryso.f

GENERIC_OBJS = \
	 pwrity.o pwry.o pwrybd.o pwrygt.o pwryso.o

SOURCES = $(GENERIC_SRCS)

OBJECTS = $(GENERIC_OBJS)

PartialLibrary(libncarg.a,$(OBJECTS))

#ifdef  BuildNCARGSourceLibrary
all:: $(SOURCES)
	@echo "$(SOURCES)" > $(MYNAME)
	@$(AR_SRC_QUICK) $(LIBDIR)/srcncarg.a $(MYNAME) $(SOURCES)
	@-$(RM) $(MYNAME)

install:: $(SOURCES)
	@echo "$(SOURCES)" > $(MYNAME)
	@$(AR_SRC_CHECK) $(LIBDIR)/srcncarg.a $(MYNAME) $(SOURCES)
	$(RANLIB) $(LIBDIR)/srcncarg.a
	@-$(RM) $(MYNAME)
#endif
