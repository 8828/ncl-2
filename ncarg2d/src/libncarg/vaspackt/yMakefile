	vtgetc.f vtgeti.f vtgetr.f vtgiws.f vtgrws.f vtinlb.f vtinrc.f \
	vtinrk.f vtmesh.f vtmviw.f vtmvrw.f vtmxyz.f vtnumb.f vtpisb.f \
	vtpitt.f vtrand.f vtrset.f vtrvsp.f vtsbst.f vtsetc.f vtseti.f \
	vtsetr.f vtsldm.f vtsldr.f vtsort.f vtsvdm.f vtsvdr.f vttdbf.f \
	vttdbm.f vttddm.f vttdfm.f vttlom.f vttmrg.f vttmtl.f vttmtx.f \
	vttpom.f vttsom.f vtvrap.f vtvrll.f

OBJECTS = \
	hluvtchcf.o hluvtchil.o hluvtchsl.o hluvtmxyz.o icavpt.o \
	icavpx.o vtabgc.o vtback.o vtblda.o vtcflb.o vtchcf.o vtchil.o \
	vtchsl.o vtcudr.o vtcvdm.o vtcvdr.o vtdrel.o vtdrpl.o vtfran.o \
	vtgetc.o vtgeti.o vtgetr.o vtgiws.o vtgrws.o vtinlb.o vtinrc.o \
	vtinrk.o vtmesh.o vtmviw.o vtmvrw.o vtmxyz.o vtnumb.o vtpisb.o \
	vtpitt.o vtrand.o vtrset.o vtrvsp.o vtsbst.o vtsetc.o vtseti.o \
	vtsetr.o vtsldm.o vtsldr.o vtsort.o vtsvdm.o vtsvdr.o vttdbf.o \
	vttdbm.o vttddm.o vttdfm.o vttlom.o vttmrg.o vttmtl.o vttmtx.o \
	vttpom.o vttsom.o vtvrap.o vtvrll.o

PartialLibraryTarget(libncarg.a,$(OBJECTS))

PartialSourceLibraryTarget($(MYNAME),srcncarg.a,$(SOURCES))

PartialLibraryTarget(libNGcn.a,$(OBJECTS))

CleanFilesTarget($(SOURCES) IftranRun)

$(SOURCES) : IftranRun

IftranRun : CodeIftran
	@$(IFTRAN) < CodeIftran > CodeFortran
	@$(FSPLIT) -both CodeFortran
	-@$(RM) CodeFortran
	touch IftranRun
