MYNAME	= ezmapa

GENERIC_SRCS = \
	mapaci.f mapbla.f mapblm.f mapchm.f mapgrm.f mapiqa.f mapiqm.f \
	mapita.f mapitm.f mprdnm.f

GENERIC_OBJS = \
	mapaci.o mapbla.o mapblm.o mapchm.o mapgrm.o mapiqa.o mapiqm.o \
	mapita.o mapitm.o mprdnm.o

SOURCES = $(GENERIC_SRCS)

OBJECTS	= $(GENERIC_OBJS)

PartialLibraryTarget(libncarg.a,$(OBJECTS))
PartialSourceLibraryTarget($(MYNAME),srcncarg.a,$(SOURCES))

PartialLibraryTarget(libNGmp.a,$(OBJECTS))
