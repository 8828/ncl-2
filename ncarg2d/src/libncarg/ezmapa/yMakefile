MYNAME	= ezmapa

GENERIC_SRCS = \
	mapaci.f mapbla.f mapblm.f mapgrm.f mapiqa.f mapiqm.f mapita.f mapitm.f

GENERIC_OBJS = \
	mapaci.o mapbla.o mapblm.o mapgrm.o mapiqa.o mapiqm.o mapita.o mapitm.o

SOURCES = $(GENERIC_SRCS)

OBJECTS	= $(GENERIC_OBJS)

PartialLibraryTarget(libncarg.a,$(OBJECTS))
PartialSourceLibraryTarget($(MYNAME),srcncarg.a,$(SOURCES))
