MYNAME  = ezmapb

GENERIC_SRCS = \
	hlumpchln.f mpchln.f mpfnme.f mpglty.f mpiaty.f mpifnb.f mpilnb.f \
	mpioar.f    mpipai.f mpipan.f mpipar.f mpisci.f mplnam.f mplndm.f \
	mplndr.f    mplnri.f mpname.f mprdcs.f

GENERIC_OBJS = \
	hlumpchln.o mpchln.o mpfnme.o mpglty.o mpiaty.o mpifnb.o mpilnb.o \
	mpioar.o    mpipai.o mpipan.o mpipar.o mpisci.o mplnam.o mplndm.o \
	mplndr.o    mplnri.o mpname.o mprdcs.o

SOURCES = $(GENERIC_SRCS)

OBJECTS	= $(GENERIC_OBJS)

PartialLibraryTarget(libncarg.a,$(OBJECTS))
PartialSourceLibraryTarget($(MYNAME),srcncarg.a,$(SOURCES))

PartialLibraryTarget(libNGmp.a,$(OBJECTS))
