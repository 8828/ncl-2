#	$Id: yMakefile,v 1.2 1992-09-29 17:17:41 ncargd Exp $
#
NCARG	= ../../../..

MYNAME	= srface

GENERIC_SRCS = \
	 srface.f srfgk.f ezsrfc.f setr.f draws.f trn32s.f clset.f ctcell.f\
	 srfabd.f

GENERIC_OBJS = \
	 srface.o srfgk.o ezsrfc.o setr.o draws.o trn32s.o clset.o ctcell.o\
	 srfabd.o

SOURCES = $(GENERIC_SRCS)

OBJECTS = $(GENERIC_OBJS)

PartialLibrary(libncarg.a,$(OBJECTS))

#ifdef  BuildNCARGSourceLibrary
all:: $(SOURCES)
	@echo "$(SOURCES)" > $(MYNAME)
	@$(AR_SRC_QUICK) $(LIBPATH)/srcncarg.a $(MYNAME) $(SOURCES)
	@-$(RM) $(MYNAME)

install:: $(SOURCES)
	@echo "$(SOURCES)" > $(MYNAME)
	@$(AR_SRC_CHECK) $(LIBPATH)/srcncarg.a $(MYNAME) $(SOURCES)
	$(RANLIB) $(LIBPATH)/srcncarg.a
	@-$(RM) $(MYNAME)
#endif
