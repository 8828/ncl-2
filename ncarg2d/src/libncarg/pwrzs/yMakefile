#
#	$Id: yMakefile,v 1.1.1.1 1992-04-17 22:31:37 ncargd Exp $
#
NCARG	= ../../../..

MYNAME	= pwrzs

GENERIC_SRCS = initzs.f intzs.f pwrzgs.f pwrzos.f pwrzs.f

GENERIC_OBJS = initzs.o intzs.o pwrzgs.o pwrzos.o pwrzs.o

SOURCES = $(GENERIC_SRCS)

OBJECTS = $(GENERIC_OBJS)

PartialLibrary(libncarg.a,$(OBJECTS))

#ifdef  BuildNCARGSourceLibrary
all:: $(SOURCES)
	@echo "$(SOURCES)" > $(MYNAME)
	@$(AR_SRC_QUICK) $(LIBDIR)/srcncarg.a $(MYNAME) $(SOURCES)
	@-$(RM) $(MYNAME)

install:: $(SOURCES)
	@echo "$(SOURCES)" > $(MYNAME)
	@$(AR_SRC_CHECK) $(LIBDIR)/srcncarg.a $(MYNAME) $(SOURCES)
	$(RANLIB) $(LIBDIR)/srcncarg.a
	@-$(RM) $(MYNAME)
#endif
