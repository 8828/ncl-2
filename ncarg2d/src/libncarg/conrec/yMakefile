#	$Id: yMakefile,v 1.2 1992-09-29 17:15:28 ncargd Exp $
#
NCARG	= ../../../..

MYNAME	= conrec

GENERIC_SRCS = \
	conrec.f clgen.f crdrln.f minmax.f reord.f stline.f calcnt.f \
	ezcntr.f conbd.f

GENERIC_OBJS = \
	conrec.o clgen.o crdrln.o minmax.o reord.o stline.o calcnt.o \
	ezcntr.o conbd.o

SOURCES = $(GENERIC_SRCS)

OBJECTS	= $(GENERIC_OBJS)

PartialLibrary(libncarg.a,$(OBJECTS))

#ifdef  BuildNCARGSourceLibrary
all:: $(SOURCES)
	@echo "$(SOURCES)" > $(MYNAME)
	@$(AR_SRC_QUICK) $(LIBPATH)/srcncarg.a $(MYNAME) $(SOURCES)
	@-$(RM) $(MYNAME)

install:: $(SOURCES)
	@echo "$(SOURCES)" > $(MYNAME)
	@$(AR_SRC_CHECK) $(LIBPATH)/srcncarg.a $(MYNAME) $(SOURCES)
	$(RANLIB) $(LIBPATH)/srcncarg.a
	@-$(RM) $(MYNAME)
#endif
