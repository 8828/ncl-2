MYNAME  = tdpackC

SRCS =  c_tdclrs.c c_tdgeti.c c_tdgetr.c c_tdgtrs.c c_tdinit.c c_tdline.c \
	c_tdlnpa.c c_tdpara.c c_tdprpa.c c_tdprpi.c c_tdseti.c c_tdsetr.c \
	c_tdstrs.c

OBJS =  c_tdclrs.o c_tdgeti.o c_tdgetr.o c_tdgtrs.o c_tdinit.o c_tdline.o \
	c_tdlnpa.o c_tdpara.o c_tdprpa.o c_tdprpi.o c_tdseti.o c_tdsetr.o \
	c_tdstrs.o

EXTRA_CDEFINES = $(OS_DEF)

PartialLibraryTarget(libncarg.a,$(OBJS))
PartialLibraryTarget(libNGmisc.a,$(OBJS))
DependTarget($(SRCS))
