NCARG	= ../../../..

MYNAME	= support

GENERIC_SRCS = \
	encd.f entsr.f retsr.f nerro.f errof.f seter.f eprin.f fdum.f \
	icloem.f seblda.f semess.f q8qst4.f mskrv1.f mskrv2.f msbsf1.f \
	msbsf2.f mssrf1.f mssrf2.f msceez.f mstrms.f msshch.f msntvl.f \
	bzbkd.f  bzcurv.f bzdp2l.f bzfcrv.f bzftol.f bzgeti.f bzgetr.f \
	bzseti.f bzsetr.f bzsubd.f idbvip.f idgrid.f idlctn.f idpdrv.f \
	idptip.f idsfft.f idtang.f idxchg.f uliber.f

GENERIC_OBJS = \
	encd.o entsr.o retsr.o nerro.o errof.o seter.o eprin.o fdum.o \
	icloem.o seblda.o semess.o q8qst4.o mskrv1.o mskrv2.o msbsf1.o \
	msbsf2.o mssrf1.o mssrf2.o msceez.o mstrms.o msshch.o msntvl.o \
	bzbkd.o bzcurv.o bzdp2l.o bzfcrv.o bzftol.o bzgeti.o bzgetr.o \
	bzseti.o bzsetr.o bzsubd.o idbvip.o idgrid.o idlctn.o idpdrv.o \
	idptip.o idsfft.o idtang.o idxchg.o uliber.o

SOURCES = $(GENERIC_SRCS)

OBJECTS = $(GENERIC_OBJS)
 
PartialLibrary(libncarg.a,$(OBJECTS))

bzbkd.o : bzcom.h
 
bzcurv.o : bzcom.h
 
bzgeti.o : bzcom.h
 
bzgetr.o : bzcom.h
 
bzseti.o : bzcom.h
 
bzsetr.o : bzcom.h

#ifdef  BuildNCARGSourceLibrary
all:: $(SOURCES)
	@echo "$(SOURCES)" > $(MYNAME)
	@$(AR_SRC_QUICK) $(LIBPATH)/srcncarg.a $(MYNAME) $(SOURCES)
	@-$(RM) $(MYNAME)

install:: $(SOURCES)
	@echo "$(SOURCES)" > $(MYNAME)
	@$(AR_SRC_CHECK) $(LIBPATH)/srcncarg.a $(MYNAME) $(SOURCES)
	$(RANLIB) $(LIBPATH)/srcncarg.a
	@-$(RM) $(MYNAME)
#endif
