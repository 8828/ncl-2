NCARG	= ../../../..

MYNAME	= fautograph

GENERIC_SRCS = fagaxclr.f fagaxlbl.f fagaxmax.f fagcuclr.f fagcudsh.f \
	fagezmxy.f fagezmy.f fagezxy.f fagezy.f fagilclr.f fagovrvw.f

install::
	@for file in $(GENERIC_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLESPATH));\
	done

MakefileTarget
CleanTarget
UpdateTarget
