NCARG	= ../../../..

MYNAME	= fthreed

GENERIC_SRCS = fthex01.f fthex02.f fthex03.f fthex04.f fthex05.f

install::
	@for file in $(GENERIC_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLESPATH));\
	done

MakefileTarget
CleanTarget
UpdateTarget
