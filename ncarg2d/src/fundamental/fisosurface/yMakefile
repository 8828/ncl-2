NCARG	= ../../../..

MYNAME	= fisosurface

GENERIC_SRCS = fiseziso.f fisissrf.f fispwrzi.f

install::
	@for file in $(GENERIC_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLESPATH));\
	done

MakefileTarget
CleanTarget
UpdateTarget
