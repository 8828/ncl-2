NCARG	= ../../../..

MYNAME	= fgks

GENERIC_SRCS = fgkgpl.f fgkgpm.f fgkgtx.f fgklnclr.f fgklnwth.f

install::
	@for file in $(GENERIC_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLESPATH));\
	done

MakefileTarget
CleanTarget
UpdateTarget
