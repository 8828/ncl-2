NCARG	= ../../../..

MYNAME	= fgks

GENERIC_SRCS = fcell.f fcell0.f fgke01.f fgke02.f fgke03.f fgke04.f fgkgpl.f \
	fgkgpm.f fgkgtx.f fgklnclr.f fgklnwth.f fgpm01.f fcirc.f

install::
	@for file in $(GENERIC_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLESPATH));\
	done

MakefileTarget
CleanTarget
UpdateTarget
