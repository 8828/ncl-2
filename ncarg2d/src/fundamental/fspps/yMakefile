NCARG	= ../../../..

MYNAME	= fspps

GENERIC_SRCS = fspcurve.f fspline.f fsppoint.f fspponts.f fcoord.f fcoord1.f \
		fcoord2.f

install::
	@for file in $(GENERIC_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLESPATH));\
	done

MakefileTarget
CleanTarget
UpdateTarget
