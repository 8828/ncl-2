NCARG	= ../../../..

MYNAME	= fspps

GENERIC_SRCS = fspcurve.f fspline.f fsppoint.f fspponts.f

install::
	@for file in $(GENERIC_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLESPATH));\
	done

MakefileTarget
CleanTarget
UpdateTarget
