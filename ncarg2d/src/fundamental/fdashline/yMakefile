NCARG	= ../../../..

MYNAME	= fdashline

GENERIC_SRCS = fdlcurvd.f fdldashc.f fdldashd.f fdlsmth.f

install::
	@for file in $(GENERIC_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$file $(EXAMPLESPATH));\
	done

MakefileTarget
CleanTarget
UpdateTarget
