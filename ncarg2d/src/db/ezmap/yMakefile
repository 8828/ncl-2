#	$Id: yMakefile,v 1.2 1992-09-29 17:03:58 ncargd Exp $
#
NCARG	= ../../../..

MYNAME	= ezmapdata

TARGETS = mapconvrt ezmapdata

all:: $(TARGETS)

mapconvrt: mapconvrt.o
	$(F77_LD) $(LD_FFLAGS) -o mapconvrt mapconvrt.o

ezmapdata: ezmapdat mapconvrt
	-@$(RM) ezmapdata
	@echo "Building the EZMAP database"
	@./mapconvrt

install::
	@$(INSTALL) -m $(INSTALL_DB) ezmapdata $(DBPATH)

clean::
	-@$(RM) $(TARGETS)

MakefileTarget
CleanTarget
