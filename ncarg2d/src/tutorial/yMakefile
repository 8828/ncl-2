NCARG	= ../../..

MYNAME	= tutorial

GENERIC_SRCS = \
	cezmap1.f cezmap2.f cezmap3.f cmpclr.f cmpcc.f cmpdd.f cmpdrw.f cmpel.f \
	cmpfil.f cmpgci.f cmpgrd.f cmpgrp.f cmpita.f cmpitm.f cmplab.f cmplbl.f \
	cmplot.f cmpmsk.f cmpou.f cmppos.f cmpsat.f cmpsup.f cmptit.f cmptra.f \
	cmpusr.f

TUTORIAL = \
	cezmap1 cezmap2 cezmap3 cmpclr cmpcc cmpdd cmpdrw cmpel \
	cmpfil cmpgci cmpgrd cmpgrp cmpita cmpitm cmplab cmplbl \
	cmplot cmpmsk cmpou cmppos cmpsat cmpsup cmptit cmptra cmpusr

all::

install:: $(GENERIC_SRCS)
	@for name in $(GENERIC_SRCS); do\
	($(INSTALL) -m $(INSTALL_LIB) $$name $(TUTORIALPATH));\
	done

MakefileTarget
CleanTarget

