MYNAME		=	libnfp

EXTRA_INCLUDES  = -I../../ncl

EXFSRCS = d1mach_sun.f
EXFOBJS = d1mach_sun.o

HDRS = wrapper.h

CSRCS = areavolW.c betaincW.c calcorcW.c chiinvW.c csagridW.c \
	cssgridW.c cz2ccmW.c datesW.c drwvctc.c dsgridW.c dtrendW.c \
	eofW.c ezfftW.c finitediffW.c fitgridW.c fluxEddyW.c hydroW.c \
	int2pW.c julGregW.c linint2W.c linmsgW.c locminmaxW.c mapgciW.c \
	natgridW.c ncolorW.c ngmiscW.c nicevalsW.c outputgif.c preshybW.c \
	probtestW.c regcoefW.c relhumW.c remapW.c rhombtriW.c shgridW.c \
	sindexW.c slpW.c skewtW.c smth9W.c sortW.c specxW.c sph2W.c sphintW.c \
	sstoiW.c statW.c svdW.c tdpackW.c trvsphW.c varimaxW.c vibetaW.c \
	wgneW.o wmapW.c wrunaveW.c wrapper.c vinth2pW.c vinth2peW.c

COBJS = areavolW.o betaincW.o calcorcW.o chiinvW.o csagridW.o \
	cssgridW.o cz2ccmW.o datesW.o drwvctc.o dsgridW.o dtrendW.o \
	eofW.o ezfftW.o finitediffW.o fitgridW.o fluxEddyW.o hydroW.o \
	int2pW.o julGregW.o linint2W.o linmsgW.o locminmaxW.o mapgciW.o \
	natgridW.o ncolorW.o ngmiscW.o nicevalsW.o outputgif.o preshybW.o \
	probtestW.o regcoefW.o relhumW.o remapW.o rhombtriW.o shgridW.o \
	sindexW.o slpW.o skewtW.o smth9W.o sortW.o specxW.o sph2W.o sphintW.o \
	sstoiW.o statW.o svdW.o tdpackW.o trvsphW.o varimaxW.o vibetaW.o \
	wgneW.o wmapW.o wrunaveW.o wrapper.o vinth2pW.o vinth2peW.o

FSRCS = aam.f areaAve.f areaRmse.f dbetai.f calcorc_dp.f chiin_dp.f \
	cntrFiniteDiff.f cz2ccm_dp.f dmapgci.f dpsort.f dtrend_dp.f ezfft_dp.f \
	fluxEddyTav_dp.f fo2fs_dp.f hydro_dp.f index77_dp.f int2p_dp.f \
	julGreg.f linint2.f linmsg_dp.f localMinMax.f phybrid_ccm.f \
	preshybrid.f prneof_dp.f prneofts_dp.f rdsstoi.f regcoef_dp.f \
	relhum_dp.f remap.f rhombtri_dp.f rmStndx_dp.f runave_dp.f \
	skewT.f smth9_dp.f sp2_util.f sp2_util_dp.f specx_dp.f statx.f \
	statx_dp.f svd_lap_dp.f svd_util_dp.f ttest.f trcwav_dp.f \
	trvsph77_dp.f upsl_dp.f varimax_dp.f vibeta_dp.f volAve.f \
	wrunave_dp.f vinth2p_dp.f vinth2p_ecmwf.f volRmse.f

FOBJS = aam.o areaAve.o areaRmse.o dbetai.o calcorc_dp.o chiin_dp.o \
	cntrFiniteDiff.o cz2ccm_dp.o dmapgci.o dpsort.o dtrend_dp.o ezfft_dp.o \
	fluxEddyTav_dp.o fo2fs_dp.o hydro_dp.o index77_dp.o int2p_dp.o \
	julGreg.o linint2.o linmsg_dp.o localMinMax.o phybrid_ccm.o \
	preshybrid.o prneof_dp.o prneofts_dp.o rdsstoi.o regcoef_dp.o \
	relhum_dp.o remap.o rhombtri_dp.o rmStndx_dp.o runave_dp.o \
	skewT.o smth9_dp.o sp2_util.o sp2_util_dp.o specx_dp.o statx.o \
	statx_dp.o svd_lap_dp.o svd_util_dp.o ttest.o trcwav_dp.o \
	trvsph77_dp.o upsl_dp.o varimax_dp.o vibeta_dp.o volAve.o \
	wrunave_dp.o vinth2p_dp.o vinth2p_ecmwf.o volRmse.o

#if defined(alpha)
EXFSRCS = d1mach_alpha.f
EXFOBJS = d1mach_alpha.o
#elif defined(linux)
EXFSRCS = d1mach_linux.f
EXFOBJS = d1mach_linux.o
#endif

OBJS = $(COBJS) $(FOBJS) $(EXFOBJS)

BuildSharedLibTarget(libNGnfp,$(OBJS),1,0,$(NCARGLIB) $(NCARGGKSLIB) $(NCARGCLIB) $(CTOFLIBS) -lm -lc)
LibraryTarget($(MYNAME),$(OBJS))
BuildIncludes($(HDRS),ncarg/nfp,../..)
DependTarget($(CSRCS))
