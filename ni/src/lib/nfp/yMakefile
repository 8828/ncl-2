MYNAME		=	libnfp

EXTRA_INCLUDES  = -I../../ncl
EXTRA_CDEFINES  = $(UDUNITS_DEFS) $(V5D_DEFS)

HDRS = wrapper.h

#ifdef BuildUdunits
UDUNITS_DEFS = -DBuildUdunits
UDUNITS_SRCS = udunitsW.c
UDUNITS_OBJS = udunitsW.o
#else
#define UDUNITS_SRCS
#define UDUNITS_OBJS
#endif

# ifdef BuildV5D
V5D_DEFS = -DBuildV5D
V5D_SRCS = ncl2v5dW.c
V5D_OBJS = ncl2v5dW.o
# else
# define    V5D_SRCS
# define    V5D_OBJS
# endif

