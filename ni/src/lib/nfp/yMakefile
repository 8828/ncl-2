EXTRA_DEFINES  = $(UDUNITS_DEFS)

HDRS = wrapper.h

#ifdef BuildUdunits
UDUNITS_DEFS = -DBuildUdunits
UDUNITS_SRCS = udunitsW.c
UDUNITS_OBJS = udunitsW.o
#else
#define UDUNITS_SRCS
#define UDUNITS_OBJS
#endif

