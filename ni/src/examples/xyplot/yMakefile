#   $Id: yMakefile,v 1.7 1995-02-09 14:57:55 haley Exp $
#
#   Makefile for HLU XYPlot examples
#

PROG_LIBS   =   $(HLULIBS) $(CNCARGLIBS) $(NCARGLIBS)
EXTRA_LIBS  =   $(XMOTIFCLIENTLIBS)
SYS_LIBS    =   $(CTOFLIBS) -lm

SRCS        = xy01c.c xy02c.c xy03c.c xy04c.c xy05c.c xy11c.c xy12c.c \
				xy13c.c xy01f.f xy02f.f xy03f.f xy11f.f xy01n.ncl xy02n.ncl \
				xy03n.ncl
OBJS        = xy01c.o xy02c.o xy03c.o xy04c.o xy05c.o xy11c.o xy12c.o \
				xy13c.o xy01f.o xy02f.o xy03f.o xy11f.o
INCLUDES    = xy13c.h 
DATA        = xy12c.asc
RES         = xy01.res xy02.res xy03.res xy04.res xy05.res
EXFILES     = $(SRCS) $(INCLUDES) $(DATA) $(RES)

InstallTarget($(EXFILES),$(INSTALL_LIB),$(HLUEXROOT))

HLUCExampleTarget(xy01c,xy01c.o)
HLUCExampleTarget(xy02c,xy02c.o)
HLUCExampleTarget(xy03c,xy03c.o)
HLUCExampleTarget(xy04c,xy04c.o)
HLUCExampleTarget(xy05c,xy05c.o)
HLUCExampleTarget(xy11c,xy11c.o)
HLUCExampleTarget(xy12c,xy12c.o)
HLUCExampleTarget(xy13c,xy13c.o)

HLUFExampleTarget(xy01f,xy01f.o)
HLUFExampleTarget(xy02f,xy02f.o)
HLUFExampleTarget(xy03f,xy03f.o)
HLUFExampleTarget(xy11f,xy11f.o)

CleanFilesTarget($(OBJS))
DependTarget($(SRCS))

