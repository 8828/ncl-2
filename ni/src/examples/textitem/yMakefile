#
#   Makefile for HLU TextItem examples
#

NCGMS   =   ti01c.ncgm ti01f.ncgm

PROG_LIBS   =   $(HLULIBS) $(CNCARGLIBS) $(NCARGLIBS)
EXTRA_LIBS  =   $(XMOTIFCLIENTLIBS)
SYS_LIBS    =   $(CTOFLIBS) -lm

SRCS        = ti01c.c ti01f.f
EXFILES     = $(SRCS)

InstallTarget($(EXFILES),$(INSTALL_LIB),$(HLUEXROOT))

HLUCExampleTarget(ti01c,ti01c.o)
HLUFExampleTarget(ti01f,ti01f.o)

CleanFilesTarget($(NCGMS))
DependTarget($(SRCS))

