NCARG = ../../../..
BINDIR_LOCAL = ../../../bin

MYNAME	= rasstat

LOCLIBDIR	= ../../../lib
LOCINCDIR	= ../../include

LIBS		= $(LIBDIR)/libncarg_ras.a $(LIBDIR)/libhdf.a \
		$(LIBDIR)/libncarv.a -lm

CDEFINES	= -I$(LOCINCDIR)

OBJS		= rasstat.o
SRCS		= rasstat.c

all::	$(MYNAME)

CTarget($(MYNAME),$(OBJS),$(LIBS))
MakefileTarget
CleanTarget

