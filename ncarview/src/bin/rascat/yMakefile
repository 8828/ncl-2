NCARG = ../../../..
BINDIR_LOCAL = ../../../bin

MYNAME	= rascat

LOCLIBDIR	= ../../../lib
LOCINCDIR	= ../../include

LIBS		= $(LIBDIR)/libncarg_ras.a $(LIBDIR)/libhdf.a \
		$(LIBDIR)/libncarv.a -lm

CDEFINES	= -I$(LOCINCDIR)

OBJS		= rascat.o
SRCS		= rascat.c

all::	$(MYNAME)

CTarget($(MYNAME),$(OBJS),$(LIBS))
MakefileTarget
CleanTarget

saber_src: $(SRCS)
