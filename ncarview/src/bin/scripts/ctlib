#
#	This script simply reports the libraries required by ctrans.
#	The library list will vary depending on machine configuration
#
#!/bin/csh

echo /u2/ncargfx/test/lib/libictrans.o /u2/ncargfx/test/lib/libctrans.o  /u2/ncargfx/test/lib/libcgm.a /u2/ncargfx/test/lib/libncarv.a -lX11  -lsuntool -lsunwindow -lpixrect -lm
