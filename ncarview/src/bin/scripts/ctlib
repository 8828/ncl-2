#
#	This script simply reports the libraries required by ctrans.
#	The library list will vary depending on machine configuration
#
#!/bin/csh

echo /tmp/testlib/libictrans.o /tmp/testlib/libctrans.o  	/tmp/testlib/libcgm.a /tmp/testlib/libncarv.a  	/tmp/testlib/libncarg_ras.a /tmp/testlib/libhdf.a -lX11   -lm
