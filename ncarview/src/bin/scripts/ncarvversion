#!/bin/csh -f
#
#	$Id: ncarvversion,v 1.9 1992-06-24 20:51:43 clyne Exp $
#

set date = `date`
set year = $date[6]

echo ""
cat <<EOF
NCAR View - UNIX Version 3.2alpha1.0
Copyright (C) $year - All Rights Reserved
University Corporation for Atmospheric Research
EOF

echo "NCAR Interactive Installed"
