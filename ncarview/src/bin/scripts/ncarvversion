#!/bin/csh -f
#
#	$Id: ncarvversion,v 1.11 1992-07-31 21:08:18 clyne Exp $
#

set date = `date`
set year = $date[6]

echo ""
cat <<EOF
NCAR View - UNIX Version 3.2alpha1.1
Copyright (C) $year - All Rights Reserved
University Corporation for Atmospheric Research
EOF

echo "NCAR Interactive Installed"
