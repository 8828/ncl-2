MYNAME	= med

EQUALHACK	= =

LOCLIB	= $(LIBDIR)/libcgm.a $(LIBDIR)/libncarg_c.a 

SRCS	= \
	cmd_tab.c commands.c error.c med.c 

OBJS	= \
	cmd_tab.o commands.o error.o med.o

#if defined(CRAY)
BSDLIBS = -lbsd
#endif

LIBS	= $(LOCLIB) $(BSDLIBS)

CDEFINES	= 

all::	$(MYNAME)

CTarget($(MYNAME),$(OBJS),$(LIBS))
MakefileTarget
CleanTarget

lint: $(HDR) $(SRCS)
	lint $(CDEFINES) $(LINTLIBS) $(SRCS)  1> lint.out 2>&1
