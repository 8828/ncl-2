NCARG	= ../..


APP	= \ 
	Cgm_edit Idt

all:

install::
	@for name in $(APP); do \
        if (test -f $(XAPPPATH)/$$name) then \
		echo "Warning - replacing $(XAPPPATH)/$$name"; \
                cp $(XAPPPATH)/$$name $(XAPPPATH)/$$name.bak ; fi; \
	($(INSTALL) -m $(INSTALL_APP) $$name.ad $(XAPPPATH)/$$name); \
	done
MakefileTarget
CleanTarget
