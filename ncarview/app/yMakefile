NCARG	= ../..


APP	= \ 
	Cgm_edit Idt

all:


install::
	@for name in $(APP); do \
        if (test -f $(XAPPDIR)/$$name) then \
		echo "Warning: Replacing $(XAPPDIR)/$$name"; \
                cp $(XAPPDIR)/$$name $(XAPPDIR)/$$name.bak ; fi; \
	($(INSTALL) -m $(INSTALL_APP) $$name.ad $(XAPPDIR)/$$name); \
	done

MakefileTarget
CleanTarget
